<Window x:Class="mainMenu.AdminFloorplan"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:mainMenu"
        mc:Ignorable="d"
        Title="AdminFloorplan Management" Height="720" Width="1280">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="87*"/>
            <ColumnDefinition Width="46*"/>
            <ColumnDefinition Width="26*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="159*"/>
            <RowDefinition Height="40*"/>
            <RowDefinition Height="490*"/>
        </Grid.RowDefinitions>

        <Border Background="BurlyWood" BorderBrush="Black" BorderThickness="2" Margin="10,1,74,10" Grid.Row="1" Grid.RowSpan="2">
            <Image x:Name="floorplanThumbnail" Margin="8,8,0,0"/>
        </Border>
        <Button x:Name="backBtn" Content="Tilbage" HorizontalAlignment="Left" Margin="18,400,0,0" VerticalAlignment="Top" Width="180" Height="80" FontSize="28" Grid.Row="2" Grid.Column="2"/>
        <Button x:Name="browseBtn" Content="Browse" HorizontalAlignment="Left" VerticalAlignment="Top" Width="107" Grid.Column="2" Height="39" Grid.Row="1" Margin="0,1,0,0"/>
        <TextBox x:Name="filepathBox" HorizontalAlignment="Left" Height="39" Margin="0,1,0,0" TextWrapping="Wrap" Text="Indtast filsti" VerticalAlignment="Top" Width="368" Grid.Row="1" Grid.Column="1" FontSize="20"/>
        <Label x:Name="windowTitle" Content="Administrer Plantegning" HorizontalAlignment="Left" Margin="426,10,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2" Height="66" Width="518" RenderTransformOrigin="0.544,-0.038" FontSize="40"/>


    </Grid>
</Window>

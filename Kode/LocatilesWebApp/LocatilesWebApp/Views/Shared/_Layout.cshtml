<body>

    <div class="container">
        <div class="row" id="navbar">

            <div class="col-xs-12 col-md-4 logoText">Locatiles</div>

            <div class="col-xs-12 col-md-4">
                <div id="searchBar">                    
                    <input type="text" id="searchfield" value="test" onkeyup="return searchbarOnKeyUp(event)" autofocus>
                    <button type="button" id="searchButton" onclick="searchItems"><span class="glyphicon glyphicon-search"></span></button>                   
                </div>
            </div>

            <div class="hidden-xs col-md-4"></div>
        </div>

        <div class="row"><div class="col-md-12 rainbowline"></div></div>

        @RenderBody()

        <div class="row" id="footer">
            <div class="col-md-12">
                <p id="footerCopyRightText">
                    Locatiles<span class="glyphicon glyphicon-copyright-mark"></span> *
                </p>
